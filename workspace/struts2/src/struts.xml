<struts>

	<constant name="struts.action.extension" value="action" />
	<!--
	<constant name="struts.locale" value="ja_JP"/>
	<constant name="struts.i18n.encoding" value="UTF-8" />
	<constant name="struts.i18n.reload" value="true" />
	-->
	
  	<package name="sample01" namespace="/sample01" extends="struts-default">
    	<action name="Hello" class="struts2.sample01.HelloAction">
	  		<result name="success">/jsp/sample01/hello.jsp</result>
		</action>
	</package>
	
  	<package name="sample04" namespace="/sample04" extends="struts-default">
		<action name="Index" class="struts2.sample04.IndexAction">
    		<result name="success">/jsp/sample04/index.jsp</result>
		</action>
		
		<action name="Textfield" class="struts2.sample04.TextfieldAction">
    		<result name="success">/jsp/sample04/textfield.jsp</result>
		</action>
		
		<action name="Checkbox" class="struts2.sample04.CheckboxAction">
    		<result name="success">/jsp/sample04/checkbox.jsp</result>
		</action>
		
		<action name="Checkboxlist" class="struts2.sample04.CheckboxlistAction">
    		<result name="success">/jsp/sample04/checkboxlist.jsp</result>
		</action>
		
